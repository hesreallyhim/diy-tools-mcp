{
  "name": "greet_user",
  "description": "Generate a greeting message",
  "language": "javascript",
  "code": "function main({ name, language = 'English' }) {\n  const greetings = {\n    English: 'Hello',\n    Spanish: 'Hola',\n    French: 'Bonjour'\n  };\n  const greeting = greetings[language] || greetings.English;\n  return `${greeting}, ${name}!`;\n}",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "User name"
      },
      "language": {
        "type": "string",
        "enum": [
          "English",
          "Spanish",
          "French"
        ],
        "description": "Language for greeting",
        "default": "English"
      }
    },
    "required": [
      "name"
    ]
  },
